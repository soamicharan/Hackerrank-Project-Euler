#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   long long int n,a,b,c,d,e,f,i,j,k;
    scanf("%llu",&e);
    for(f=1;f<=e;f++)
        {
    scanf("%llu",&n);
    n=n-1;
    i=n/3;j=n/5;k=n/15;
    a=(i*(6+(i-1)*3))/2;
    b=(j*(10+(j-1)*5))/2;
    c=(k*(30+(k-1)*15))/2;
    d=a+b-c;
    printf("%llu\n",d);
    }
    return 0;
}
